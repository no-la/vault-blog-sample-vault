views:
  - type: table
    name: Table
    filters:
      and:
        - file.folder == "Posts"
    order:
      - file.name
      - tags
    sort:
      - property: createdAt
        direction: ASC
  - type: table
    name: View
    filters:
      and:
        - file.folder == "NolaBlog"
    sort:
      - property: createdAt
        direction: ASC
